- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to t('devise.sign_up'), new_registration_path(resource_name), class: 'default-orange'
  %br/
- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  = link_to t('devise.did_not_reseive_unlock_instructions'), new_unlock_path(resource_name)
  %br/
